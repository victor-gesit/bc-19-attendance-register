<% include ../partials/head%>
<div class='container'>
	<div class='timer'>
	<!-- Count if inputted by JavaScript -->
		<p id='count'><%= count %></p>
	</div>
	<form method='post' >
		<div id='reg'>
			<p class='event'><%= eventName%></p>
			<hr>
			<div class='info'>
				<p>Name</p>
				<input type='text' name='name' id='name'>
			</div>

			<div class='info'>
				<p>Email</p>
				<input type='email' name='email' id='email'>
			</div>
			<input type='submit' name='submit' value='Submit' onclick='addRow();'>
		</div>
	</form>

	<table id='table'>
		<thead>
			<tr id='theading'>
				<td>
					Time-in
				</td>
				<td>
					Name
				</td>
				<td>
					Email
				</td>
			</tr>
		</thead>
		<tbody id='table-body'>
			<% for(var i=0; i < data.length; i++) { %>
   			<tr>
     			<td><%= data[i].timeIn %></td>
     			<td><%= data[i].name %></td>
     			<td><%= data[i].email%></5d>
   			</tr>
   			<% } %>
		</tbody>
	</table>
</div>

<% include ../partials/footer%>