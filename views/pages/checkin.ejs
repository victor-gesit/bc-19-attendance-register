<div class='container'>
	<div class='timer'>
		<p id='count'><%=count%></p>
	</div>
	<form method='post' >
		<div id='reg'>
			<p class='event'><%=eventName %></p>
			<hr>
			<div class='info'>
				<p>Name</p>
				<input type='text' name='name' id='name'>
			</div>

			<div class='info'>
				<p>Email</p>
				<input type='email' name='email' id='email'>
			</div>
			<input type='submit' name='submit' value='Submit'>
		</div>
	</form>
<%=data%>
	<table id='table'>
		<thead>
			<tr id='theading'>
				<td>
					Time-in
				</td>
				<td>
					Name
				</td>
				<td>
					Email
				</td>
			</tr>
		</thead>
		<tbody id='table-body'>
		
		</tbody>
	</table>
</div>